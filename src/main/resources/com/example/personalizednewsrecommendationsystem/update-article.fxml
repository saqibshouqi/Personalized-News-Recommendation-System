<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>




<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx/fxml"
            fx:controller="com.example.personalizednewsrecommendationsystem.UpdateArticle"
            prefHeight="400.0" prefWidth="600.0">

<VBox spacing="10" layoutX="100" layoutY="50">
    <ChoiceBox fx:id="categoryChoiceBox" />
    <TextField fx:id="titleField" promptText="Title" />
    <TextArea fx:id="contentField" promptText="Content" prefHeight="200" />
    <Button text="Update" onAction="#onUpdateClick" />
</VBox>
</AnchorPane>
